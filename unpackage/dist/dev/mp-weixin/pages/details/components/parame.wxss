/* 透明背景动画 */
.anim.data-v-b57d604a {
	-webkit-animation: implement-data-v-b57d604a;
	        animation: implement-data-v-b57d604a;
	/*动画名称*/
	-webkit-animation-duration: 0.4s;
	        animation-duration: 0.4s;
	/*动画持续时间*/
}
@-webkit-keyframes implement-data-v-b57d604a {
0% {
		opacity: 0;
}
50% {
		opacity: 0.5;
}
100% {
		opacity: 1;
}
}
@keyframes implement-data-v-b57d604a {
0% {
		opacity: 0;
}
50% {
		opacity: 0.5;
}
100% {
		opacity: 1;
}
}
/* 底部淡入 */
.coup-anim.data-v-b57d604a {
	-webkit-animation: fadeIn-data-v-b57d604a 0.4s .2s ease both;
	        animation: fadeIn-data-v-b57d604a 0.4s .2s ease both;
}
@-webkit-keyframes fadeIn-data-v-b57d604a {
0% {
		opacity: 0;
		-webkit-transform: translateY(400rpx);
		        transform: translateY(400rpx)
}
100% {
		opacity: 1;
		-webkit-transform: translateY(0);
		        transform: translateY(0)
}
}
@keyframes fadeIn-data-v-b57d604a {
0% {
		opacity: 0;
		-webkit-transform: translateY(400rpx);
		        transform: translateY(400rpx)
}
100% {
		opacity: 1;
		-webkit-transform: translateY(0);
		        transform: translateY(0)
}
}
